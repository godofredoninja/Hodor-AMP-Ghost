    {{!-- Header --}}
    <header class="main-header u-bgColor u-container">
        <nav class="blog-title u-flexCenter">
            {{!-- <a href="{{@blog.url}}" class="logo-link" >
                <span class="logo-img">
                    <amp-img src="{{@blog.logo}}" width="110" height="30" alt="{{@blog.title}}"> </amp-img>
                </span>
            </a> --}}

            {{!-- name of Page (Logo) --}}
            <a href="{{@blog.url}}" class="logo-link u-flex1">{{@blog.title}}</a>

            {{!-- Open Sidenav --}}
            <button class="hamburgermenu" on="tap:sidebar1.toggle" tabindex="0"><span></span><span></span><span></span></button>
        </nav>
    </header>

    <main class="main">

        {{#post}}

        <article class="article u-container">
            <header class="article-header">

                <div class="u-flexCenter">

                {{#author}}

                    {{!-- Author avatar --}}
                    <div class="author u-overflowHidden">
                        <a href="{{url absolute="true"}}"  class="u-relative u-block author-avatar">
                            <amp-img class="u-absolute author-image" src="{{#if profile_image}} {{img_url profile_image absolute="true"}} {{else}} {{asset "images/avatar.png" absolute="true"}} {{/if}}" width="60" height="60" alt="{{name}}"> </amp-img>
                        </a>
                    </div>

                    <div class="author-left u-flex1 u-overflowHidden">
                        {{!-- Author Name --}}
                        <a href="{{url absolute="true"}}">{{name}}</a>

                {{/author}}

                        {{!-- Author biography --}}
                        {{#if author.bio}}<div class="author-bio u-block u-overflowHidden u-fontSizeSmaller u-textColorNormal">{{{author.bio}}}</div>{{/if}}

                        {{!-- Datetime of post --}}
                        <time class="datetime u-textColorNormal u-fontSizeSmaller u-block u-textColorNormal" datetime="{{date format='YYYY-MM-DD'}}">{{date published_at timeago="true"}}</time>
                    </div>
                </div>

                {{!-- Title of article --}}
                <h1 class="article-title">{{title}}</h1>

                {{!-- Excerpt --}}
                {{#if custom_excerpt}}<p class="article-excerpt">{{custom_excerpt}}</p>{{/if}}
            </header>

            {{!-- featured image --}}
            {{#if feature_image}}
                <figure class="article-image u-block u-marginTop30">
                    <amp-img src="{{img_url feature_image absolute="true"}}" width="600" height="400" layout="responsive"></amp-img>
                </figure>
            {{/if}}

            {{!-- article body (all content) --}}
            <div class="article-body"> {{amp_content}} </div>

            {{!-- Post All Tags (category) --}}
            {{#if tags}}<div class="article-tags">{{#foreach tags}}<a href="{{url absolute="true"}}" title="{{name}}" class="button" >{{name}}</a>{{/foreach}}</div>{{/if}}

            {{!-- Share social Media --}}
            <div class="share">
                <ul>
